<h2>New Celebrity</h2>
    <form method="POST" action="/celebrities/create">
        <div class="inputform">
            <label for="name">Name</label>
            <input type="text" class="form1" id="name" name="name">
        </div>
        <div class="inputform">
            <label for="occupation">Occupation</label>
            <input type="text" class="form1" id="occupation" name="occupation">
        </div>
        <div class="inputform">
            <label for="catchPhrase">CatchPhrase</label>
            <input type="text" class="form1" id="catchPhrase" name="catchPhrase">
        </div>
        <button type="submit" class="button">Submit</button>
    </form>